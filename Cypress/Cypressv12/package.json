{
  "name": "cypressv12",
  "version": "1.0.0",
  "description": "E2E Automated Tests using Cypress v12",
  "author": "Adrian Ling",
  "license": "ISC",
  "main": "index.js",
  "repository": "https://github.com/ajling/E2EAutomation",
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true
  },
  "resolutions": {
    "cypress": "12.4.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:ci": "start-server-and-test client:dev http-get://localhost:8080 cy:run_smoketests",
    "cy:run_smoketests": "yarn cypress run --browser chrome --headless -e TAGS=\"@smokeTests\"",
    "cy:open": "yarn cypress open --e2e --browser chrome",
    "cy:run": "yarn cypress run --browser electron --spec 'cypress/e2e/**/*.feature'"
  },
  "dependencies": {
    "@babel/core": "7.20.12",
    "@badeball/cypress-cucumber-preprocessor": "^15.1.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@cypress/browserify-preprocessor": "3.0.2",
    "@cypress/xpath": "^2.0.3",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "async": "3.2.4",
    "chai": "4.3.7",
    "chai-sorted": "0.2.0",
    "concurrently": "7.6.0",
    "cypress": "^12.4.1",
    "cypress-failed-log": "2.10.0",
    "cypress-file-upload": "5.0.8",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.2.3",
    "cypress-multi-reporters": "^1.6.2",
    "cypress-wait-until": "1.7.2",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "2.8.3",
    "serve": "14.2.0",
    "start-server-and-test": "1.15.3",
    "strip-ansi": "^7.0.1",
    "url-parse": "1.5.10",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {}
}
